<foxml:datastream ID="modsToUketddcXSL" STATE="A" CONTROL_GROUP="X" VERSIONABLE="false">
<foxml:datastreamVersion ID="modsToUketddcXSL.0" LABEL="Mods to UKETD_DC XSL" CREATED="2011-12-06T10:59:27.038Z" MIMETYPE="text/xml" SIZE="5313">
<foxml:xmlContent>
<xsl:stylesheet xmlns:mods="http://www.loc.gov/mods/v3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" exclude-result-prefixes="mods" version="1.0">
  <xsl:param name="pid"></xsl:param>
  <xsl:preserve-space elements="*"></xsl:preserve-space>
  <xsl:output encoding="UTF-8" indent="yes" media-type="text/xml" method="xml"></xsl:output>
  <xsl:template match="/">
    <xsl:variable name="descMetadata" select="//parent::mods:mods"></xsl:variable>
    <uketd_dc:uketddc xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:uketd_dc="http://naca.central.cranfield.ac.uk/ethos-oai/2.0/" xmlns:uketdterms="http://naca.central.cranfield.ac.uk/ethos-oai/terms/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <dc:title>
        <xsl:value-of select="$descMetadata/mods:titleInfo/mods:title/text()"></xsl:value-of>
      </dc:title>
      <xsl:for-each select="$descMetadata/mods:name[@type=&apos;personal&apos;]">
        <xsl:choose>
          <xsl:when test="contains(./mods:role/mods:roleTerm/text(), &apos;creator&apos;)">
            <dc:creator>
              <xsl:value-of select="./mods:namePart/text()"></xsl:value-of>
            </dc:creator>
          </xsl:when>
          <xsl:when test="contains(./mods:role/mods:roleTerm/text(), &apos;Supervisor&apos;)">
            <uketdterms:advisor>
              <xsl:value-of select="concat(./mods:namePart/text(), &apos; (Supervisor)&apos;)"></xsl:value-of>
            </uketdterms:advisor>
          </xsl:when>
        </xsl:choose>
      </xsl:for-each>
      <xsl:for-each select="$descMetadata/mods:name[@type=&apos;corporate&apos;]">
        <xsl:if test="contains(./mods:role/mods:roleTerm/text(), &apos;Sponsor&apos;)">
          <uketdterms:sponsor>
            <xsl:value-of select="concat(./mods:namePart/text(), &apos; (Sponsor)&apos;)"></xsl:value-of>
          </uketdterms:sponsor>
        </xsl:if>
        <xsl:if test="contains(./mods:role/mods:roleTerm/text(), &apos;sponsor&apos;)">
          <uketdterms:sponsor>
            <xsl:value-of select="concat(./mods:namePart/text(), &apos; (Sponsor)&apos;)"></xsl:value-of>
          </uketdterms:sponsor>
        </xsl:if>
      </xsl:for-each>
      <dcterms:issued>
        <xsl:value-of select="$descMetadata/mods:originInfo/mods:dateIssued/text()"></xsl:value-of>
      </dcterms:issued>
      <xsl:for-each select="$descMetadata/mods:subject/mods:topic">
        <dc:subject>
          <xsl:value-of select="text()"></xsl:value-of>
        </dc:subject>
      </xsl:for-each>
      <xsl:if test="$descMetadata/mods:identifier[@type=&apos;grantNumber&apos;]">
        <xsl:for-each select="$descMetadata/mods:identifier[@type=&apos;grantNumber&apos;]">
          <uketdterms:grantnumber>
            <xsl:value-of select="text()"></xsl:value-of>
          </uketdterms:grantnumber>
        </xsl:for-each>
      </xsl:if>
      <dcterms:abstract>
        <xsl:value-of select="$descMetadata/mods:abstract/text()"></xsl:value-of>
      </dcterms:abstract>
      <dc:date>
        <xsl:value-of select="$descMetadata/mods:recordInfo/mods:recordCreationDate/text()"></xsl:value-of>
      </dc:date>
      <dc:format xsi:type="dcterms:IMT">
        <xsl:value-of select="$descMetadata/mods:physicalDescription/mods:internetMediaType/text()"></xsl:value-of>
      </dc:format>
      <dc:language xsi:type="dcterms:ISO6392">
        <xsl:value-of select="$descMetadata/mods:language/mods:languageTerm[@type=&apos;code&apos;]/text()"></xsl:value-of>
      </dc:language>
      <dc:type>
        <xsl:value-of select="$descMetadata/mods:genre/text()"></xsl:value-of>
      </dc:type>
      <uketdterms:qualificationlevel>
        <xsl:value-of select="$descMetadata/mods:note[@type=&apos;qualificationLevel&apos;]/text()"></xsl:value-of>
      </uketdterms:qualificationlevel>
      <uketdterms:qualificationname>
        <xsl:value-of select="$descMetadata/mods:note[@type=&apos;qualificationName&apos;]/text()"></xsl:value-of>
      </uketdterms:qualificationname>
      <uketdterms:institution>
        <xsl:value-of select="normalize-space(substring-after($descMetadata/mods:originInfo/mods:publisher, &apos;,&apos;))"></xsl:value-of>
      </uketdterms:institution>
      <uketdterms:department>
        <xsl:value-of select="normalize-space(substring-before($descMetadata/mods:originInfo/mods:publisher, &apos;,&apos;))"></xsl:value-of>
      </uketdterms:department>
      <xsl:for-each select="$descMetadata/mods:accessCondition">
        <dc:rights>
          <xsl:value-of select="text()"></xsl:value-of>
        </dc:rights>
      </xsl:for-each>
      <dcterms:isReferencedBy xsi:type="dcterms:URI">
        <xsl:value-of select="$descMetadata/mods:location/mods:url[@access=&apos;object in context&apos;]/text()"></xsl:value-of>
      </dcterms:isReferencedBy>
      <dc:identifier xsi:type="dcterms:URI">
        <xsl:value-of select="$descMetadata/mods:location/mods:url[@access=&apos;raw object&apos;]/text()"></xsl:value-of>
      </dc:identifier>
    </uketd_dc:uketddc>
  </xsl:template>
</xsl:stylesheet>
</foxml:xmlContent>
</foxml:datastreamVersion>
</foxml:datastream>
